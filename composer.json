{
  "name": "webanet-au/slim4",
  "type": "project",
  "description": "Slim PHP Micro-Framework skeleton using ADR pattern with Doctrine",
  "keywords": [
    "slim",
    "framework",
    "boilerplate",
    "skeleton",
    "doctrine"
  ],
  "homepage": "https://github.com/webanet-au/Slim4",
